<!DOCTYPE html>
<html>
  <head>
    <title> Hospital List </title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
  </head>
  <body>

    <section id="header">
      <a href="#"><img src="images/Hlogo.jpg" class="logo"></a>
    </section>   
    
    
    <% hospitalList.forEach((info)=>{ %>
      <!-- card1 -->

    <div class="card" style="width: 18rem; margin-top: 20px; margin-left: 20px;">
        <img src="../image/lumbinitechnical.jpg" class="card-img-top" alt="lumbini technical HospitalDetail">
        <div class="card-body">
          <h5 class="card-title"><%= info.hospitalName %></h5>
          <a href="./hospitaldetail/<%= info._id %>" class="btn btn-primary">Book a Appointment</a>
        </div>
    </div>
      <% }) %>
       
   
   
    


  </body>
</html>
